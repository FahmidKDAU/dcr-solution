{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "dcr-form-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/dcrForm/DcrFormWebPart.js",
          "manifest": "./src/webparts/dcrForm/DcrFormWebPart.manifest.json"
        }
      ]
    },
    "task-hub-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/taskHub/TaskHubWebPart.js",
          "manifest": "./src/webparts/taskHub/TaskHubWebPart.manifest.json"
        }
      ]
    },
    "document-portal-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/documentPortal/DocumentPortalWebPart.js",
          "manifest": "./src/webparts/documentPortal/DocumentPortalWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "DcrFormWebPartStrings": "lib/webparts/dcrForm/loc/{locale}.js",
    "TaskHubWebPartStrings": "lib/webparts/taskHub/loc/{locale}.js",
    "DocumentPortalWebPartStrings": "lib/webparts/documentPortal/loc/{locale}.js"
  }
}
